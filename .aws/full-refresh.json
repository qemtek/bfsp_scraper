{
    "family": "full-refresh",
    "networkMode": "awsvpc",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512",
    "executionRoleArn": "arn:aws:iam::249959970268:role/ecsTaskExecutionRole",
    "taskRoleArn": "arn:aws:iam::249959970268:role/ecsTaskRole",
    "containerDefinitions": [
        {
            "name": "full-refresh",
            "image": "249959970268.dkr.ecr.eu-west-1.amazonaws.com/bfsp_scraper/full-refresh:latest",
            "essential": true,
            "cpu": 256,
            "memory": 512,
            "memoryReservation": 256,
            "environment": [
                {
                    "name": "COUNTRIES",
                    "value": "uk,ire,fr"
                },
                {
                    "name": "TYPES",
                    "value": "win,place"
                },
                {
                    "name": "S3_BUCKET",
                    "value": "betfair-sp"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/full-refresh",
                    "awslogs-region": "eu-west-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ]
}
